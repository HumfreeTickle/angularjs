{
    "name": "comicapp_v2",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "build": "npm run build:css && tsc -p src/ ",
        "build:watch": "tsc -p src/ -w",
        "watch:css": "onchange 'src/scss/*.scss' -- npm run build:css",
        "build:e2e": "tsc -p e2e/",
        "scss": "node-sass --output-style compressed -o src/.tmp/ src/app/styles/ ",
        "autoprefixer": "postcss -u autoprefixer -r  src/.tmp/*",
        "build:css": "npm run scss && npm run autoprefixer",
        "serve": "lite-server -c=bs-config.json",
        "serve:e2e": "lite-server -c=bs-config.e2e.json",
        "prestart": "npm run build",
        "start": "concurrently \"npm run build:watch\" \"npm run serve\" ",
        "pree2e": "npm run build:e2e",
        "e2e": "concurrently \"npm run serve:e2e\" \"npm run protractor\" --kill-others --success first",
        "preprotractor": "webdriver-manager update",
        "protractor": "protractor protractor.config.js",
        "pretest": "npm run build",
        "test": "concurrently \"npm run build:watch\" \"karma start karma.conf.js\"",
        "pretest:once": "npm run build",
        "test:once": "karma start karma.conf.js --single-run",
        "lint": "tslint ./src/**/*.ts -t verbose"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/HumfreeTickle/angularjs.git"
    },
    "author": "stephen larkin",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/HumfreeTickle/angularjs/issues"
    },
    "homepage": "https://github.com/HumfreeTickle/angularjs#readme",
    "dependencies": {
        "@angular/common": "~4.0.0",
        "@angular/compiler": "~4.0.0",
        "@angular/core": "~4.0.0",
        "@angular/forms": "~4.0.0",
        "@angular/http": "~4.0.0",
        "@angular/platform-browser": "~4.0.0",
        "@angular/platform-browser-dynamic": "~4.0.0",
        "@angular/router": "~4.0.0",
        "systemjs": "0.19.40",
        "core-js": "^2.4.1",
        "rxjs": "5.0.1",
        "zone.js": "^0.8.4"
    },
    "devDependencies": {
        "@types/jasmine": "2.5.36",
        "@types/node": "^6.0.46",
        "autoprefixer": "^6.7.7",
        "canonical-path": "0.0.2",
        "concurrently": "^3.2.0",
        "jasmine-core": "~2.4.1",
        "karma": "^1.3.0",
        "karma-chrome-launcher": "^2.0.0",
        "karma-cli": "^1.0.1",
        "karma-jasmine": "^1.0.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "lite-server": "^2.2.2",
        "lodash": "^4.16.4",
        "node-sass": "^4.5.2",
        "nodemon": "^1.11.0",
        "postcss-cli": "^3.0.0",
        "protractor": "~4.0.14",
        "rimraf": "^2.5.4",
        "tslint": "^3.15.1",
        "typescript": "~2.1.0"
    }
}